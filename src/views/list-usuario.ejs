<%-include('partials/_header-buscador');%>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <table class="table table-bordered table-hover">
                <thead>
                  <tr>
                    <th>Nombre</th>
                    <th>Cedula</th>
                    <th>Telefono</th>
                    <th>Direccion</th>
                    <th>Fecha de nacimiento</th>
                    <th>Genero</th>
                    <th>Cliente</th>
                    <th>Sede</th>
                    <th>Editar</th>
                    <th>Eliminar</th>
                  </tr>  
                </thead>
                <tbody>
                   
                        <tr>
                            <td> <%= usuario.nombre %></td>
                            <td> <%= usuario.cedula %></td>
                            <td> <%= usuario.telefono%></td>
                            <td> <%= usuario.direccion %></td>
